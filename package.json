{
  "name": "graphql-ts-server",
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migration:run": "typeorm migration:run -d dist/data-source.js",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate src/migrations/AddNameToExercise -d src/data-source.ts"
  },
  "dependencies": {
    "apollo-server": "^3.13.0",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.1",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "graphql-type-json": "^0.3.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "^2.0.0-rc.2"
  },
  "devDependencies": {
    "@types/node": "^24.0.13",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.25",
    "typescript": "^5.4.0"
  }
}
